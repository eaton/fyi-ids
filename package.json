{
  "name": "@eatonfyi/ids",
  "version": "1.2.0",
  "description": "Generate, validate, and pretty-print a variety of IDs and hash formats.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "demo": "node ./scripts/demo.mjs",
    "build": "pkgroll",
    "package": "pkgroll --clean-dist --minify",
    "test": "tap"
  },
  "keywords": [
    "asin",
    "gtin",
    "isbn",
    "ulid",
    "uuid",
    "hash"
  ],
  "author": "Jeff Eaton <jeff@eaton.fyi> (https://eaton.fyi)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eaton/ids.git"
  },
  "devDependencies": {
    "@tapjs/typescript": "^3.0.0",
    "@types/node": "^20.17.9",
    "pkgroll": "^2.5.1",
    "tap": "^21.0.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@sindresorhus/fnv1a": "^3.1.0",
    "@types/object-hash": "^3.0.6",
    "bufferbase": "^1.3.0",
    "gtin": "^1.0.2",
    "isbn3": "^1.2.3",
    "nanoid": "^5.0.9",
    "object-hash": "^3.0.0",
    "ulidx": "^2.4.1",
    "uuid": "^11.0.3",
    "uuid25": "^0.1.5"
  }
}
